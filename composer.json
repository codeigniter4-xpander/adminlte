{
    "name": "codeigniter4-xpander/adminlte",
    "description": "AdminLTE using codeigniter 4 xpander",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagus A Priambada",
            "email": "iam.adty@gmail.com"
        }
    ],
    "require": {
        "codeigniter4-xpander/core": "dev-develop",
        "oomphinc/composer-installers-extender": "^1.1",
        "npm-asset/admin-lte": "^2.4",
        "danielstjules/stringy": "^3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CI4Xpander_AdminLTE\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "installer-types": ["npm-asset"],
        "installer-paths": {
            "public/assets/npm/{$name}/": ["type:npm-asset"]
        }
    }
}
